        "\n",
        "fib1 = fib2 = 1\n",
        " \n",
        "n = input(\"Номер элемента ряда Фибоначчи: \")\n",
        "n = int(n) - 2\n",
        " \n",
        "while n > 0:\n",
        "    fib1, fib2 = fib2, fib1 + fib2\n",
        "    n -= 1\n",
        " \n",
        "print(\"Значение этого элемента:\", fib2)"
